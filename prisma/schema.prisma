// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  id String @id
  email String @unique
  fullname String
  password String
  transport_reference Json?
}

model Transports {
  id Int @id @default(autoincrement())
  brand_name String
  model String
}